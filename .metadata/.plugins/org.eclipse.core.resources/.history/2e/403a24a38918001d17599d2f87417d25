package projectA13;

public class projectA1313 {

	public static void main(String[] args) {
		StringBuffer sb = new StringBuffer("This");
		
		sb.append(" is pencel");
		System.out.println(sb);
		
		sb.insert(8, "my");
		System.out.println(sb);
		
		sb.replace(8, 11, "your");
		System.out.println(sb);
		
		sb.delete(8, 13);
		System.out.println(sb);
		
		sb.setLength(4);
		System.out.println(sb);
		
	}

}
