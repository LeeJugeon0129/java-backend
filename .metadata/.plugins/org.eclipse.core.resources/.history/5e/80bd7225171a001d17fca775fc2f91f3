package projectA15;

public class projectA1511 {

class Person{
	private int weight;
	int age;
	protected int height;
	public String name;
	
	public void setWeight(int weight) {
		this.weight = weight;
	}
	public int getWeight() {
		return weight;
	}
}
	
	
public class InheritanceEx{
	public static void main(String[] args) {
		Student s = new Student();
		s.set();
	}

}
}
