package projectA13;

interface Calculatable{
	public int sum();
}
class GoodCals{
	private int field;
	public void method(int...value) {
		
		Calculatable calc = new Calculatable() {
			public int sum() {
				int sum=0;
				for(int i:value) {
					sum = sum + i;
				}
				return sum;
			}
		};
		System.out.println(calc.sum());
		
	}
}

public class projectA1309 {

	public static void main(String[] args) {
		
	}

}
