package projectA15;

import java.util.HashMap;

public class projectA1506 {

	public static void main(String[] args) {
		HashMap<String, String> dic = new HashMap<>(); //HashMap<key, value> - 인덱스에 빠르게 접근
		
		dic.put("baby", "아기");
		dic.put("love", "사랑");
		dic.put("apple", "사과");
		
		
		
		
	}

}
